{
  "compilerOptions": {
    "outDir": "../dist/client/",
    "noImplicitAny": true,
    "module": "ES6",
    "target": "ESNext",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    },
  }
}