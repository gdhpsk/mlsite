  "scripts": {
    "build:server": "webpack --config server/webpack.config.cjs",
    "build:client": "npx tailwindcss -c ./client/tailwind.config.js -i ./client/src/style.css -o ./dist/client/style.css --minify && webpack --config client/webpack.config.cjs",
    "build": "yarn build:server && yarn build:client",
    "start": "node dist/server/bundle.js",
    "dev:build": "nodemon --config client/nodemon.json",
    "dev:start": "node --watch dist/server/bundle.js",
    "lint:server": "npx prettier server/src --write",
    "lint:client": "prettier --write 'client/src/**/*.{ts,tsx,css}' --config ./client/.prettierrc",
    "lint": "yarn lint:server && yarn lint:client",
    "test:db": "jest ./server --env=node --detectOpenHandles -i --verbose"
  },
